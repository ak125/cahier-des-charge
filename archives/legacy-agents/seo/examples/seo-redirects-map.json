{
    "/core/seo/blog.php": {
        "target": "/blog",
        "type": 301
    },
    "/core/seo/article.php": {
        "target": "/blog/articles",
        "type": 301
    },
    "/core/products/view.php": {
        "target": "/produits",
        "type": 301
    },
    "/services/detail.php": {
        "target": "/services",
        "type": 301
    },
    "/ancienne-page": {
        "target": "/nouvelle-page",
        "type": 301
    },
    "/page-supprimee.php": {
        "target": "",
        "type": 410
    },
    "/page-conditionnelle.php": {
        "target": "/page-alternative",
        "type": 412
    },
    "/regex/produit/(.*).html": {
        "target": "/produits/$1",
        "type": 301,
        "regex": true
    },
    "/old/category/(.*)/page/(\\d+)": {
        "target": "/categorie/$1?page=$2",
        "type": 301,
        "regex": true
    }
}