{
  "name": "migration-ai-pipeline",
  "version": "1.0.0",
  "description": "Pipeline IA pour la migration de code legacy PHP vers TypeScript/Remix/NestJS",
  "main": "index.js",
  "scripts": {
    "setup": "task setup",
    "generate-agent-configs": "node scripts/generate-agent-configs.js",
    "test-agents": "node scripts/test-agents.js",
    "agent-status": "task agents:status",
    "setup-n8n": "task n8n:import",
    "export-workflows": "node scripts/export-workflows.js",
    "test-workflows": "node scripts/test-workflows.js",
    "setup-github-hooks": "node scripts/setup-github-hooks.js",
    "setup-github-actions": "node scripts/setup-github-actions.js",
    "verify-installation": "node scripts/verify-installation.js",
    "diagnostic": "node scripts/diagnostic.js",
    "migration-test": "node scripts/migration-test.js",
    "check-migration-results": "node scripts/check-migration-results.js",
    "version": "echo \"Migration AI Pipeline v1.0.0\"",
    "update-to-version": "node scripts/update-to-version.js",
    "generate-docs": "node scripts/generate-docs.js",
    "generate-procedure-pdf": "node scripts/generate-procedure-pdf.js",
    "export-config-template": "node scripts/export-config-template.js",
    "logs": "task docker:logs",
    "advanced-diagnostic": "node scripts/advanced-diagnostic.js",
    "health-check": "task audit:temporal",
    "check-tech": "node scripts/check-tech-obsolescence.js",
    "update-tech": "node scripts/update-tech.js",
    "generate-audit": "task audit",
    "watch-migrations": "node scripts/migration-watcher.js",
    "version-cdc": "node scripts/version-manager.js",
    "archive-version": "node scripts/archive-version.js",
    "scheduled-version": "node scripts/scheduled-versioning.js",
    "add-journal-entry": "node scripts/add-journal-entry.js",
    "install-hooks": "cp scripts/git-hook-journal.js .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "generate-view": "node scripts/generate-html-view.js",
    "orchestrate": "task migrate:batch",
    "audit": "task audit",
    "audit-dir": "ts-node migration-orchestrator.ts audit-dir",
    "cahier": "ts-node migration-orchestrator.ts cahier",
    "migration-status": "task migrate:status",
    "migration-dashboard": "ts-node scripts/dashboard.js",
    "unified-audit": "ts-node agents/agent-audit.ts",
    "validate-config": "ts-node scripts/validate-migration-config.js",
    "n8n:setup": "task n8n:start",
    "n8n:start": "task n8n:start",
    "n8n:stop": "docker-compose -f docker-compose.n8n.yml down",
    "n8n:migrate": "./scripts/migration/run-migration.sh",
    "n8n:logs": "docker-compose -f docker-compose.n8n.yml logs -f n8n",
    "mcp-verify": "task agents:run -- mcp-verifier",
    "bullmq:start": "task agents:bullmq-orchestrator",
    "bullmq:stop": "kill $(ps aux | grep 'ts-node.*bullmq' | awk '{print $2}')",
    "post-generate": "ts-node scripts/post-generate-verification.js",
    "start:worker": "ts-node src/temporal/worker.ts",
    "start:client": "ts-node src/temporal/client.ts",
    "transform": "ts-node src/temporal/client.ts",
    "format": "biome format --write .",
    "lint": "eslint --config .eslintrc.json \"**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "eslint --config .eslintrc.json \"**/*.{ts,tsx}\" --fix",
    "lint:ci": "biome ci .",
    "build:earthly": "earthly +ci",
    "build:remix": "earthly +remix-build",
    "build:nestjs": "earthly +nestjs-build",
    "build:images": "earthly +deploy",
    "dev": "task dev",
    "test": "task test",
    "classify:structure": "node tools/structure/structure-classifier-loader.js",
    "classify:structure-plan": "ts-node --project ts-node.json tools/structure/structure-refactor-planner.ts",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint",
    "validate:lenient": "npm run type-check && npm run lint -- --max-warnings=9999"
  },
  "dependencies": {
    "@bull-board/api": "^6.9.1",
    "@bull-board/express": "^6.9.1",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/event-emitter": "^2.0.2",
    "@octokit/rest": "^18.12.0",
    "@sentry/node": "^7.93.0",
    "@temporalio/client": "^1.11.7",
    "@temporalio/common": "^1.11.7",
    "@temporalio/worker": "^1.11.7",
    "@temporalio/workflow": "^1.11.7",
    "@types/diff": "^7.0.2",
    "axios": "^1.6.0",
    "bullmq": "^4.13.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "diff": "^7.0.0",
    "dotenv": "^16.3.1",
    "eventsource": "^3.0.6",
    "execa": "^7.2.0",
    "express": "^4.18.2",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "langchain": "^0.1.4",
    "mongoose": "^7.5.0",
    "openai": "^4.0.0",
    "ora": "^5.4.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "semver": "^7.5.4",
    "table": "^6.8.1",
    "ts-morph": "^25.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@eslint/eslintrc": "^3.3.1",
    "@prisma/client": "^6.6.0",
    "@supabase/supabase-js": "^2.39.3",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/execa": "^2.0.0",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.4",
    "@types/redis": "^4.0.11",
    "@types/table": "^6.3.2",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@typescript-eslint/typescript-estree": "^8.30.1",
    "cheerio": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "marked": "^15.0.8",
    "minimatch": "^10.0.1",
    "minimist": "^1.2.8",
    "php-parser": "^3.1.5",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "zod": "^3.24.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-organisation/migration-ai-tools.git"
  },
  "author": "",
  "license": "MIT",
  "typesVersions": {
    "*": {
      "php-parser": [
        "./@types/php-parser/index.d.ts"
      ]
    }
  }
}