      "main": [
        [
          {
            "node": "Prepare Files List",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Run mysql-analyzer": {
      "main": [
        [
          {
            "node": "Prepare Files List",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Prepare Files List": {
      "main": [
        [
          {
            "node": "Process Files Individually",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Process Files Individually": {
      "main": [
        [
          {
            "node": "Run php-analyzer-v2",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Run php-analyzer-v2": {
      "main": [
        [
          {
            "node": "Run sync-mapper",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
